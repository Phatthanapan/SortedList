<Window x:Class="Lecture_86.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lecture_86"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="ketTxt" HorizontalAlignment="Left" Height="43" Margin="67,44,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="160"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="43" Margin="67,113,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="160"/>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Center" Height="112" Margin="0,44,0,0" VerticalAlignment="Top" Width="195" Click="addBtn_Click"/>

    </Grid>
</Window>
